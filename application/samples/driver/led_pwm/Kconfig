config LED_PWM_PIN
    int
    prompt "Choose pwm pin."
    default 4
config PWM_CHANNEL
    int
    prompt "Choose PWM Test Channel."
    depends on SAMPLE_SUPPORT_PWM
    default 4

config PWM_GROUP_ID
    int
    prompt "Choose PWM Test Group ID."
    depends on SAMPLE_SUPPORT_PWM && PWM_USING_V151
    default 0

config PWM_PIN_MODE
    int
    prompt "Choose PWM pin mode."
    default 1
    depends on SAMPLE_SUPPORT_PWM

