config SAMPLE_MAIN_TASK1_LED
   bool"Suppose sample main task1 led."
    depends on ENABLE_MAIN_SAMPLE
    help
        This option means support sample main task1 led.

if SAMPLE_MAIN_TASK1_LED
menu "Task1 LED Configuration"
    osource "application/samples/main/task1_led/Kconfig"
    endmenu
endif

